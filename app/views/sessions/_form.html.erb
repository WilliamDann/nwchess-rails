<%= form_with(model: session) do |form| %>
    <div>
      <%= form.label :email, style: "display: block" %>
      <%= form.text_field :email %>
    </div>
  
    <div>
      <%= form.label :password, style: "display: block" %>
      <%= form.password_field :password %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  